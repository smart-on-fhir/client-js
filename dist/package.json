{
  "name": "fhirclient",
  "version": "3.0.0-beta.1",
  "description": "JavaScript client for Fast Healthcare Interoperability Resources",
  "author": "SMART Health IT <support@smarthealthit.org> (https://smarthealthit.org/)",
  "contributors": [
    "Josh Mandel <joshua.mandel@childrens.harvard.edu>",
    "Vladimir Ignatov <vlad.ignatov@gmail.com>",
    "Dylan Phelan <dtphelan1@gmail.com>"
  ],
  "keywords": [
    "SMART",
    "FHIR",
    "Client"
  ],
  "license": "Apache-2.0",

  "exports": {
    ".": {
      "types": "./index.d.ts"
    },
    "./browser": {
      "import": "./esm/entry/browser.js",
      "require": "./cjs/entry/browser.js",
      "types": "./types/entry/browser.d.ts"
    },
    "./node": {
      "import": "./esm/entry/node.js",
      "require": "./cjs/entry/node.js",
      "types": "./types/entry/node.d.ts"
    },
    "./hapi": {
      "import": "./esm/entry/hapi.js",
      "require": "./cjs/entry/hapi.js",
      "types": "./types/entry/hapi.d.ts"
    },
    "./*": {
      "import": "./esm/*.js",
      "require": "./cjs/*.js",
      "types": "./types/*.d.ts"
    }
  },
  "dependencies": {
    "debug": "^4.1.1",
    "jose": "^4.15.5",
    "js-base64": "^3.7.2"
  },
  "peerDependencies": {
    "@types/hapi": ">=18",
    "@types/node": ">=18"
  },
  "peerDependenciesMeta": {
    "@types/hapi": {
      "optional": true
    },
    "@types/node": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smart-on-fhir/client-js.git"
  },
  "bugs": {
    "url": "https://github.com/smart-on-fhir/client-js/issues"
  },
  "homepage": "http://docs.smarthealthit.org/client-js/"
}
